<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="simple" targetRuntime="MyBatis3Simple">

   <jdbcConnection driverClass="${driverClass}" connectionURL="${connectionURL}" userId="${userId}" password="${password}">
   <property name="nullCatalogMeansCurrent" value="true" />
   </jdbcConnection>

     <javaModelGenerator targetPackage="${modelPackage}" targetProject="${targetProject}"/>
     <javaClientGenerator targetPackage="${mapperPackage}" targetProject="${targetProject}" type="ANNOTATEDMAPPER"/>


    <table schema="KIDDA_LA" tableName="%" />
  </context>
</generatorConfiguration>